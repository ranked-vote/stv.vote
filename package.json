{
  "name": "rcv.report",
  "description": "Detailed reports on ranked-choice elections",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "RANKED_VOTE_REPORTS=report_pipeline/reports vite dev",
    "build": "RANKED_VOTE_REPORTS=report_pipeline/reports vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate-images": "node scripts/generate-share-images.mjs",
    "report": "cd report_pipeline && ./report.sh",
    "report:sync": "cd report_pipeline && ./sync.sh",
    "report:extract": "cd report_pipeline && ./extract-from-archives.sh",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --ci --coverage --maxWorkers=2"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "tippy.js": "^6.3.7",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@tsconfig/svelte": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "puppeteer": "^22.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^3.6.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
