{
  "name": "ranked.vote",
  "description": "Detailed reports on ranked-choice elections",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "RANKED_VOTE_REPORTS=report_pipeline/reports vite dev",
    "prebuild": "svelte-kit sync",
    "build": "RANKED_VOTE_REPORTS=report_pipeline/reports vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate-images": "bun scripts/generate-share-images.mjs",
    "report": "cd report_pipeline && ./report.sh",
    "report:sync": "cd report_pipeline && ./sync.sh",
    "report:extract": "cd report_pipeline && ./extract-from-archives.sh",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:ci": "bun test --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.2",
    "tippy.js": "^6.3.7",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^25.0.0",
    "husky": "^9.1.7",
    "puppeteer": "^24.32.1",
    "svelte": "^5.45.8",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  }
}
